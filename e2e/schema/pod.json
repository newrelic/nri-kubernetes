{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "clusterName": {
      "$id": "#/properties/clusterName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "test"
      ]
    },
    "createdAt": {
      "$id": "#/properties/createdAt",
      "type": "integer",
      "minimum": 1600000000,
      "examples": [
        1621608424
      ]
    },    "createdBy": {
      "$id": "#/properties/createdBy",
      "type": "string",
      "minLength": 1,
      "examples": [
        "coredns-f9fd979d6"
      ]
    },
    "createdKind": {
      "$id": "#/properties/createdKind",
      "type": "string",
      "minLength": 1,
      "examples": [
        "ReplicaSet"
      ]
    },
    "deploymentName": {
      "$id": "#/properties/deploymentName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "coredns"
      ]
    },
    "displayName": {
      "$id": "#/properties/displayName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "coredns-f9fd979d6-qt56l"
      ]
    },
    "event_type": {
      "$id": "#/properties/event_type",
      "type": "string",
      "const": "K8sPodSample"
    },
    "isReady": {
      "$id": "#/properties/isReady",
      "type": "integer",
      "minimum": 0,
      "maximum": 1,
      "examples": [
        1
      ]
    },
    "isScheduled": {
      "$id": "#/properties/isScheduled",
      "type": "integer",
      "minimum": 0,
      "maximum": 1,
      "examples": [
        1
      ]
    },
    "namespace": {
      "$id": "#/properties/namespace",
      "type": "string",
      "examples": [
        "kube-system"
      ]
    },
    "namespaceName": {
      "$id": "#/properties/namespaceName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "kube-system"
      ]
    },
    "net.errorsPerSecond": {
      "$id": "/properties/net.errorsPerSecond",
      "type": "integer"
    },
    "net.rxBytesPerSecond": {
      "$id": "/properties/net.rxBytesPerSecond",
      "type": "number"
    },
    "net.txBytesPerSecond": {
      "$id": "/properties/net.txBytesPerSecond",
      "type": "number"
    },
    "nodeIP": {
      "$id": "#/properties/nodeIP",
      "type": "string",
      "format": "ipv4",
      "examples": [
        "192.168.49.2"
      ]
    },
    "podIP": {
      "$id": "#/properties/podIP",
      "type": "string",
      "format": "ipv4",
      "examples": [
        "172.17.0.6"
      ]
    },
    "nodeName": {
      "$id": "#/properties/nodeName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "minikube"
      ]
    },
    "podName": {
      "$id": "#/properties/podName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "coredns-f9fd979d6-qt56l"
      ]
    },
    "startTime": {
      "$id": "#/properties/startTime",
      "type": "integer",
      "minimum": 1600000000,
      "examples": [
        1621608442
      ]
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "minLength": 1,
      "examples": [
        "Running"
      ]
    }
  },
  "examples": [
    {
      "clusterName": "test",
      "createdAt": 1621608439,
      "createdBy": "coredns-f9fd979d6",
      "createdKind": "ReplicaSet",
      "deploymentName": "coredns",
      "displayName": "coredns-f9fd979d6-qt56l",
      "event_type": "K8sPodSample",
      "isReady": 1,
      "isScheduled": 1,
      "namespace": "kube-system",
      "namespaceName": "kube-system",
      "nodeIP": "192.168.49.2",
      "podIP": "172.17.0.6",
      "nodeName": "minikube",
      "podName": "coredns-f9fd979d6-qt56l",
      "startTime": 1621608442,
      "status": "Running"
    }
  ],
  "required": [
    "clusterName",
    "displayName",
    "event_type",
    "isReady",
    "isScheduled",
    "namespace",
    "namespaceName",
    "net.errorsPerSecond",
    "net.rxBytesPerSecond",
    "net.txBytesPerSecond",
    "podIP",
    "nodeIP",
    "nodeName",
    "podName",
    "startTime",
    "status"
  ]
}

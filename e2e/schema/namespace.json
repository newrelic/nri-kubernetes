{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "clusterName": {
      "$id": "#/properties/clusterName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "test"
      ]
    },
    "createdAt": {
      "$id": "#/properties/createdAt",
      "type": "integer",
      "minimum": 1600000000,
      "examples": [
        1621608424
      ]
    },
    "displayName": {
      "$id": "#/properties/displayName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "kubernetes"
      ]
    },
    "event_type": {
      "$id": "#/properties/event_type",
      "type": "string",
      "const": "K8sNamespaceSample"
    },
    "namespace": {
      "$id": "#/properties/namespace",
      "type": "string",
      "minLength": 1,
      "examples": [
        "kube-system"
      ]
    },
    "namespaceName": {
      "$id": "#/properties/namespaceName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "kube-system"
      ]
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "minLength": 1,
      "examples": [
        "Active"
      ]
    }
  },
  "examples": [
    {
      "clusterName": "test",
      "createdAt": 1621608421,
      "displayName": "kube-system",
      "event_type": "K8sNamespaceSample",
      "namespace": "kube-system",
      "namespaceName": "kube-system",
      "status": "Active"
    }
  ],
  "required": [
    "clusterName",
    "createdAt",
    "displayName",
    "event_type",
    "namespace",
    "namespaceName",
    "status"
  ]
}

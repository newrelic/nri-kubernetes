{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "clusterName": {
      "$id": "#/properties/clusterName",
      "type": "string",
      "examples": [
        "test"
      ]
    },
    "createdAt": {
      "$id": "#/properties/createdAt",
      "type": "integer",
      "minimum": 1600000000,
      "examples": [
        1621608501
      ]
    },
    "deploymentName": {
      "$id": "#/properties/deploymentName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "ksm-kube-state-metrics"
      ]
    },
    "displayName": {
      "$id": "#/properties/displayName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "ksm-kube-state-metrics-6b8c5674b6"
      ]
    },
    "event_type": {
      "$id": "#/properties/event_type",
      "type": "string",
      "const": "K8sReplicasetSample"
    },
    "namespace": {
      "$id": "#/properties/namespace",
      "type": "string",
      "minLength": 1,
      "examples": [
        "nr"
      ]
    },
    "namespaceName": {
      "$id": "#/properties/namespaceName",
      "type": "string",
      "examples": [
        "nr"
      ]
    },
    "podsDesired": {
      "$id": "#/properties/podsDesired",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "podsFullyLabeled": {
      "$id": "#/properties/podsFullyLabeled",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "podsMissing": {
      "$id": "#/properties/podsMissing",
      "type": "integer",
      "examples": [
        0
      ]
    },
    "podsReady": {
      "$id": "#/properties/podsReady",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "podsTotal": {
      "$id": "#/properties/podsTotal",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "replicasetName": {
      "$id": "#/properties/replicasetName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "ksm-kube-state-metrics-6b8c5674b6"
      ]
    }
  },
  "examples": [
    {
      "clusterName": "test",
      "createdAt": 1621608501,
      "deploymentName": "ksm-kube-state-metrics",
      "displayName": "ksm-kube-state-metrics-6b8c5674b6",
      "event_type": "K8sReplicasetSample",
      "namespace": "nr",
      "namespaceName": "nr",
      "podsDesired": 1,
      "podsFullyLabeled": 1,
      "podsMissing": 0,
      "podsReady": 1,
      "podsTotal": 1,
      "replicasetName": "ksm-kube-state-metrics-6b8c5674b6"
    }
  ],
  "required": [
    "clusterName",
    "createdAt",
    "deploymentName",
    "displayName",
    "event_type",
    "namespace",
    "namespaceName",
    "observedGeneration",
    "podsDesired",
    "podsFullyLabeled",
    "podsMissing",
    "podsReady",
    "podsTotal",
    "replicasetName"
  ]
}

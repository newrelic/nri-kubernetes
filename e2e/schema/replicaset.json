{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "clusterName": {
      "$id": "/properties/clusterName",
      "type": "string",
      "examples": [
        "ohai-k8s-dev"
      ]
    },
    "createdAt": {
      "$id": "/properties/createdAt",
      "type": "integer",
      "examples": [
        1519221397
      ]
    },
    "deploymentName": {
      "$id": "/properties/deploymentName",
      "type": "string",
      "examples": [
        "dns-controller"
      ]
    },
    "displayName": {
      "$id": "/properties/displayName",
      "type": "string",
      "examples": [
        "dns-controller-6c864dbb6f"
      ]
    },
    "entityName": {
      "$id": "/properties/entityName",
      "type": "string",
      "examples": [
        "k8s:ohai-k8s-dev:kube-system:replicaset:dns-controller-6c864dbb6f"
      ]
    },
    "event_type": {
      "$id": "/properties/event_type",
      "type": "string",
      "examples": [
        "K8sReplicasetSample"
      ]
    },
    "namespace": {
      "$id": "/properties/namespace",
      "type": "string",
      "examples": [
        "kube-system"
      ]
    },
    "namespaceName": {
      "$id": "/properties/namespaceName",
      "type": "string",
      "minLength": 1
    },
    "observedGeneration": {
      "$id": "/properties/observedGeneration",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "podsDesired": {
      "$id": "/properties/podsDesired",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "podsFullyLabeled": {
      "$id": "/properties/podsFullyLabeled",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "podsReady": {
      "$id": "/properties/podsReady",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "podsTotal": {
      "$id": "/properties/podsTotal",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "replicasetName": {
      "$id": "/properties/replicasetName",
      "type": "string",
      "examples": [
        "dns-controller-6c864dbb6f"
      ]
    }
  },
  "required": [
    "clusterName",
    "createdAt",
    "deploymentName",
    "displayName",
    "entityName",
    "event_type",
    "namespace",
    "namespaceName",
    "observedGeneration",
    "podsDesired",
    "podsFullyLabeled",
    "podsReady",
    "podsTotal",
    "replicasetName"
  ]
}
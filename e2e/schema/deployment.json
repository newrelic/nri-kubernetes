{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "clusterName": {
      "$id": "#/properties/clusterName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "test"
      ]
    },
    "createdAt": {
      "$id": "#/properties/createdAt",
      "type": "integer",
      "minimum": 1600000000,
      "examples": [
        1621608424
      ]
    },
    "deploymentName": {
      "$id": "#/properties/deploymentName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "coredns"
      ]
    },
    "displayName": {
      "$id": "#/properties/displayName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "coredns"
      ]
    },
    "entityName": {
      "$id": "#/properties/entityName",
      "type": "string",
      "minLength": 4,
      "examples": [
        "k8s:test:kube-system:deployment:coredns"
      ]
    },
    "event_type": {
      "$id": "#/properties/event_type",
      "type": "string",
      "const": "K8sDeploymentSample"
    },
    "namespace": {
      "$id": "#/properties/namespace",
      "type": "string",
      "examples": [
        "kube-system"
      ]
    },
    "namespaceName": {
      "$id": "#/properties/namespaceName",
      "type": "string",
      "examples": [
        "kube-system"
      ]
    },
    "podsAvailable": {
      "$id": "#/properties/podsAvailable",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "podsDesired": {
      "$id": "#/properties/podsDesired",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "podsMaxUnavailable": {
      "$id": "#/properties/podsMaxUnavailable",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "podsMissing": {
      "$id": "#/properties/podsMissing",
      "type": "integer",
      "examples": [
        0
      ]
    },
    "podsTotal": {
      "$id": "#/properties/podsTotal",
      "type": "integer",
      "examples": [
        1
      ]
    },
    "podsUnavailable": {
      "$id": "#/properties/podsUnavailable",
      "type": "integer",
      "examples": [
        0
      ]
    },
    "podsUpdated": {
      "$id": "#/properties/podsUpdated",
      "type": "integer",
      "examples": [
        1
      ]
    }
  },
  "examples": [
    {
      "clusterName": "test",
      "createdAt": 1621608424,
      "deploymentName": "coredns",
      "displayName": "coredns",
      "entityName": "k8s:test:kube-system:deployment:coredns",
      "event_type": "K8sDeploymentSample",
      "namespace": "kube-system",
      "namespaceName": "kube-system",
      "podsAvailable": 1,
      "podsDesired": 1,
      "podsMaxUnavailable": 1,
      "podsMissing": 0,
      "podsTotal": 1,
      "podsUnavailable": 0,
      "podsUpdated": 1
    }
  ],
  "required": [
    "clusterName",
    "createdAt",
    "deploymentName",
    "displayName",
    "entityName",
    "event_type",
    "namespace",
    "namespaceName",
    "podsAvailable",
    "podsDesired",
    "podsMaxUnavailable",
    "podsMissing",
    "podsTotal",
    "podsUnavailable",
    "podsUpdated"
  ]
}

{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "clusterIP": {
      "$id": "#/properties/clusterIP",
      "type": "string",
      "examples": [
        "10.96.0.1"
      ]
    },
    "clusterName": {
      "$id": "#/properties/clusterName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "test"
      ]
    },
    "createdAt": {
      "$id": "#/properties/createdAt",
      "type": "integer",
      "minimum": 1600000000,
      "examples": [
        1621608424
      ]
    },
    "displayName": {
      "$id": "#/properties/displayName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "kubernetes"
      ]
    },
    "entityName": {
      "$id": "#/properties/entityName",
      "type": "string",
      "minLength": 4,
      "examples": [
        "k8s:test:default:service:kubernetes"
      ]
    },
    "event_type": {
      "$id": "#/properties/event_type",
      "type": "string",
      "const": "K8sServiceSample"
    },
    "externalName": {
      "$id": "#/properties/externalName",
      "type": "string",
      "examples": [
        ""
      ]
    },
    "namespaceName": {
      "$id": "#/properties/namespaceName",
      "type": "string",
      "examples": [
        "default"
      ]
    },
    "serviceName": {
      "$id": "#/properties/serviceName",
      "type": "string",
      "minLength": 1,
      "examples": [
        "kubernetes"
      ]
    },
    "specType": {
      "$id": "#/properties/specType",
      "type": "string",
      "minLength": 1,
      "examples": [
        "ClusterIP"
      ]
    }
  },
  "examples": [
    {
      "clusterIP": "10.96.0.1",
      "clusterName": "test",
      "createdAt": 1621608423,
      "displayName": "kubernetes",
      "entityName": "k8s:test:default:service:kubernetes",
      "event_type": "K8sServiceSample",
      "externalName": "",
      "namespaceName": "default",
      "serviceName": "kubernetes",
      "specType": "ClusterIP"
    }
  ],
  "required": [
    "clusterName",
    "createdAt",
    "displayName",
    "entityName",
    "event_type",
    "externalName",
    "namespaceName",
    "serviceName",
    "specType"
  ]
}
